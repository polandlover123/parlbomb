{
  "layer": "top",
  "position": "top",
  "height": 21,
  "modules-left": [
    "user",
    "custom/cap-left#lighterblue",
    "group/windows",
    "custom/cap-left#dark",
    "custom/gap",
    "network",
    "bluetooth",
    "custom/cap-left",
    "battery",
    "group/audio",
    "custom/cap-left-end",
    "hyprland/submap"
  ],
  "modules-right": [
    "privacy",
    "custom/gap-right#dark",
    "network#right",
    "custom/cap-right#dark",
    "clock#clock2",
    "custom/cap-right",
    "clock"
  ],
  "group/audio": {
    "orientation": "horizontal",
    "modules": [
      "wireplumber"
    ]
  },
  "mpris": {
    "format": "{title} - {artist} player - {player}"
  },
  "wireplumber": {
    "format": "{icon} {volume}%",
    "format-muted": "ÔÄ¶",
    "on-click": "helvum",
    "format-icons": [
      "ÔÄ¶",
      "ÔÄß",
      "ÔÄ®"
    ]
  },
  "privacy": {
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 24
      },
      {
        "type": "audio-out",
        "tooltip": true,
        "tooltip-icon-size": 24
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 24
      }
    ]
  },
  "backlight/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal",
    "device": "intel_backlight"
  },
  "user": {
    "format": "~/zsh::{work_M}"
  },
  "battery": {
    "bat": "BAT0",
    "min-length": 5,
    "states": {
      "full": 95,
      "warning": 30,
      "critical": 15
    },
    "tooltip": false,
    "full-at": 90,
    "format-plugged": "{icon} {capacity}%",
    "format-charging": "{icon} {capacity}%",
    "format": "{icon} {capacity}%",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ]
  },
  "group/submap": {
    "orientation": "horizontal",
    "children-class": "submap-child",
    "modules": [
      "hyprland/submap",
      "custom/cap-left"
    ]
  },
  "group/windows": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 500,
      "transition-left-to-right": false,
      "click-to-reveal": true
    },
    "modules": [
      "hyprland/workspaces",
      "hyprland/window"
    ]
  },
  "custom/lock": {
    "format": "Û∞çÅ",
    "tooltip": false,
    "on-click": "hyprlock"
  },
  "custom/power": {
    "format": "ÔÄë",
    "tooltip": false,
    "on-click": "systemctl shutdown"
  },
  "hyprland/workspaces": {
    "active-only": true,
    "format": "[{id}]"
  },
  "hyprland/window": {
    "format": "{title}"
  },
  "clock": {
    "format": "ÔÅ≥ {:%Y-%m-%d}",
    "tooltip": false
  },
  "clock#clock2": {
    "format": "ÔÄó {:%I:%M %p}",
    "tooltip": false
  },
  "network": {
    "format-wifi": "Ôá´  {essid}",
    "format-ethernet": "ÔÉ® {ipaddr}",
    "format-linked": "ÔÉÅ {ipaddr}",
    "format-disconnected": "ÔÅ± Disconnected",
    "format-disabled": "",
    "tooltip": false,
    "max-length": 21,
    "min-length": 6
  },
  "network#right": {
    "format": "{bandwidthDownBytes}"
  },
  "pulseaudio": {
    "tooltip": false,
    "min-length": 6,
    "format": "{icon} {volume}%",
    "format-bluetooth": "{icon} {volume}%",
    "format-muted": "MUTE",
    "format-icons": {
      "headphone": "ÔÄ•",
      "headset": "Ôäî",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "scroll-step": 1
  },
  "temperature": {
    "tooltip": false,
    "min-length": 5,
    "critical-threshold": 90,
    "format": "{icon} {temperatureC}¬∞C",
    "format-critical": "{icon} {temperatureC}¬∞C",
    "format-icons": {
      "default": [
        "Ôãã",
        "Ôãä",
        "Ôãâ",
        "Ôãà",
        "Ôãá"
      ]
    }
  },
  "memory": {
    "tooltip": false,
    "format": "Ôãõ {percentage}%",
    "states": {
      "critical": 90
    },
    "min-length": 5
  },
  "disk": {
    "tooltip": false,
    "path": "/",
    "interval": 60,
    "min-length": 5,
    "format": "ÔÅª {percentage_used}%",
    "states": {
      "critical": 90
    }
  },
  "custom/cap-left": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/cap-left#dark": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/cap-left#lighterblue": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/cap-left#dark-orange": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/cap-left-end": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/cap-right": {
    "tooltip": false,
    "format": "ÓÇ≤"
  },
  "custom/cap-right#dark": {
    "tooltip": false,
    "format": "ÓÇ≤"
  },
  "custom/gap": {
    "tooltip": false,
    "format": "ÓÇ∞"
  },
  "custom/gap#endcap": {
    "tooltip": false,
    "format": "üØ•"
  },
  "custom/gap-right": {
    "tooltip": false,
    "format": "ÓÇ≤"
  },
  "custom/gap-right#dark": {
    "tooltip": false,
    "format": "ÓÇ≤"
  },
  "custom/logo": {
    "format": "{}",
    "exec": "echo 'Ôåì '"
  },
  "hyprland/submap": {
    "format": " {}",
    "tooltip": false,
    "always-on": true,
    "default-submap": "NORMAL"
  }
}
